<?xml version="1.0" encoding="UTF-8"?>
<!-- profile skeleton -->
<LEDSpicer
	version="1.0"
	type="Profile"
	backgroundColor=""
>
	<!-- Sample start transition -->
	<startTransition
		type="Pulse"
		color="White"
		speed="Fast"
		filter="Combine"
		direction="ForwardBouncing"
	/>
	<!-- Sample end transition -->
	<endTransition
		type="Pulse"
		color="White"
		speed="Fast"
		filter="Combine"
		direction="ForwardBouncing"
	/>
	<!-- Define one or more animation files to run -->
	<animations>
		<animation name="" />
	</animations>
	<!-- Defines overwrites for elements backgrond, always on color -->
	<alwaysOnElements>
		<element name="" color=""/>
	</alwaysOnElements>
	<!-- Defines overwrites for groups backgrond, always on color -->
	<alwaysOnGroups>
		<group name="" color=""/>
	</alwaysOnGroups>
</LEDSpicer>
